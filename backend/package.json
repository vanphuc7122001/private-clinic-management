    "@types/cors": "^2.8.16",
    "@types/formidable": "^3.4.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.201",
    "@types/mime": "^3.0.4",
    "@types/morgan": "^1.9.9",
    "@types/uuid": "^9.0.7",
